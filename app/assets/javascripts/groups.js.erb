$('.user .foot .online-courses, .user .foot .github, .user .foot .blog-posts').tooltip({delay: { show: 500, hide: 100 }});

var series_interval = <%= 2 %>;


$(function() {
  new Highcharts.Chart({


    chart: {
      renderTo: "user_chart"

    },
    title: {
      text: "users"
    },
    xAxis: {
      type: "time"
    },
    yAxis: {
      title: {
        text: "stars"
      }
    },
    series: [{
      pointInterval: series_interval,
      name: 'one',
      data: [1, 0, 4, 5, 6, 7]
    }, {
      pointInterval: series_interval,
      name: 'two',
      data: [9, 4, 5, 0, 0, 7]
    }]
  });
});